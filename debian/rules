#!/usr/bin/make -f
export DH_VERBOSE=1
DEB_HOST_MULTIARCH ?= $(shell dpkg-architecture -qDEB_HOST_MULTIARCH)

%:
	dh $@ --with autoreconf	--parallel

override_dh_auto_configure:
	dh_auto_configure -- --with-snappy --with-io_lib --with-gnutls --with-nettle --with-yasm --with-lzma --disable-ssse3 --disable-sse4
